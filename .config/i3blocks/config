[battery]
command=~/.config/i3blocks/battery/battery
label=BAT 
interval=30

[memory]
command=~/.config/i3blocks/memory/memory
label=MEM 
interval=30

[volume-pipewire]
command=~/.config/i3blocks/volume-pipewire/volume-pipewire
interval=once
signal=1

[time]
command=~/.config/i3blocks/time/time
interval=1
STRFTIME_FORMAT=%a %b %d %H:%M:%S

